<p id="notice"><%= notice %></p>

<h1>Listing Finalgroups</h1>


<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Finalposition</th>
      <th>Startup</th>
      <th>Is winner?</th>
      <th colspan="4"></th>
    </tr>
  </thead>


  <tbody>
    <% @finalgroups.order(finalposition_id: :asc).each do |finalgroup| %>
      <tr>
        <td><%= "#{Finalposition.find(finalgroup.finalposition).name}"%></td>
        <td><%= "#{Startup.find(finalgroup.startup).name}"%></td>
         <td><%= finalgroup.iswinner %></td>
        <td><%= link_to 'Show', finalgroup %></td>
        <td><%= link_to 'Edit', edit_finalgroup_path(finalgroup) %></td>
        <td><%= link_to 'Destroy', finalgroup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Finalgroup', new_finalgroup_path %>
