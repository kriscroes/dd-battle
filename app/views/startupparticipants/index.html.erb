<p id="notice"><%= notice %></p>

<h1>Listing Startupparticipants</h1>

<table>
  <thead>
    <tr>
      <th>Startup</th>
      <th>Participant</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @startupparticipants.each do |startupparticipant| %>
      <tr>
        <td><%= "#{Startup.find(startupparticipant.startup).name}"%></td>
        <td><%= "#{Participant.find(startupparticipant.participant).name}"%></td>
        <td><%= startupparticipant.role %></td>
        <td><%= link_to 'Show', startupparticipant %></td>
        <td><%= link_to 'Edit', edit_startupparticipant_path(startupparticipant) %></td>
        <td><%= link_to 'Destroy', startupparticipant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Startupparticipant', new_startupparticipant_path %>
